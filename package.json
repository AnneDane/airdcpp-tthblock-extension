{
  "name": "airdcpp-tthblock-extension",
  "version": "1.20.56",
  "description": "TTH Blocker Extension for AirDC++",
  "license": "MIT",
  "author": {
    "name": "xAI Grok"
  },
  "keywords": [
    "airdcpp",
    "airdcpp-extensions",
    "airdcpp-extensions-public",
    "airdcpp-extensions-test"
  ],
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "start": "node devtools/watch.js ../dist/main.js",
    "prepublishOnly": "npm run build",
    "webpack": "webpack"
  },
  "dependencies": {
    "airdcpp-apisocket": "^3.0.0-beta.14",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/runtime": "^7.15.3",
    "airdcpp-extension": "^1.5.1",
    "airdcpp-extension-settings": "^1.2.1",
    "babel-loader": "^8.2.5",
    "core-js": "^3.16.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.19",
    "webpack": "^5.101.0",
    "webpack-cli": "^4.10.0"
  },
  "main": "dist/main.js",
  "airdcpp": {
    "apiVersion": 1,
    "minApiFeatureLevel": 0,
    "signalReady": false,
    "hasSettings": true,
    "auto_start": true
  }
}
